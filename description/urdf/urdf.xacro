<!-- Right wheel -->

<link name="right_motor_mount">
  <visual>
    <origin xyz="${10 * 0.001} ${3.375 * 0.001} ${0 * 0.001}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find cobalt)/description/meshes/motor_mount.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="black"/>
  </visual>
</link>

<joint name="right_motor_mount_joint" type="fixed">
  <origin xyz="${-10 * 0.001} ${-32 * 0.001} 0" rpy="0 0 0"/>
  <parent link="chassis"/>
  <child link="right_motor_mount"/>
</joint>

<link name="right_wheel">
  <visual>
    <origin xyz="0 0 ${5 * 0.001}" rpy="${pi/2} 0 0"/>
    <geometry>
      <mesh filename="file://$(find cobalt)/description/meshes/tire.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="yellow"/>
  </visual>
</link>

<joint name="right_wheel_joint" type="continuous">
  <origin xyz="${10 * 0.001} ${(-3.125 - 0.5) * 0.001} 0" rpy="0 0 0"/>
  <parent link="right_motor_mount"/>
  <child link="right_wheel"/>
  <axis xyz="0.0 1.0 0.0"/>
</joint>

