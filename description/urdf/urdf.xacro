<!-- Caster wheel -->

<link name="caster_wheel">
  <visual>
    <origin xyz="${42 * 0.001} ${12 * 0.001} ${-11.7 * 0.001}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find cobalt)/description/meshes/caster_wheel.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="black"/>
  </visual>
  <collision>
    <origin xyz="${42 * 0.001} ${12 * 0.001} ${-11.7 * 0.001}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find cobalt)/description/meshes/caster_wheel.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="caster_wheel_joint" type="fixed">
  <origin xyz="${-42 * 0.001} ${-12 * 0.001} 0" rpy="0 0 0"/>
  <parent link="chassis"/>
  <child link="caster_wheel"/>
</joint>

