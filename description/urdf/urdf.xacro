<?xml version="1.0" ?>
<robot name="urdf" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find cobalt)/description/urdf/inertia.xacro" />
<xacro:include filename="$(find cobalt)/description/urdf/materials.xacro" />
<xacro:include filename="$(find cobalt)/description/urdf/urdf.trans" />
<xacro:include filename="$(find cobalt)/description/urdf/urdf.gazebo" />

<!-- Base link -->

<link name="base_link">
</link>

<!-- Chassis -->

<link name="chassis">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find cobalt)/description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="${1.7 * 0.001}" radius="${50 * 0.001}"/>
      </geometry>
    </collision>
</link>

<joint name="chassis_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chassis"/>
    <origin xyz="0 0 0"/>
</joint>

<!-- Left wheel -->

<link name="left_motor_mount">
  <visual>
    <origin xyz="${10 * 0.001} ${3.375 * 0.001} ${0 * 0.001}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find cobalt)/description/meshes/motor_mount.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="black"/>
  </visual>
</link>

<joint name="left_motor_mount_joint" type="fixed">
  <origin xyz="${10 * 0.001} ${32 * 0.001} 0" rpy="0 0 ${pi}"/>
  <parent link="chassis"/>
  <child link="left_motor_mount"/>
</joint>

<link name="left_wheel">
  <visual>
    <origin xyz="0 0 ${5 * 0.001}" rpy="${pi/2} 0 0"/>
    <geometry>
      <mesh filename="file://$(find cobalt)/description/meshes/tire.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="yellow"/>
  </visual>
  <collision>
    <origin xyz="0 ${-5 * 0.001} ${5 * 0.001}" rpy="${pi/2} 0 0"/>
    <geometry>
      <cylinder length="${10 * 0.001}" radius="${21 * 0.001}"/>
    </geometry>
  </collision>
</link>

<joint name="left_wheel_joint" type="continuous">
  <origin xyz="${10 * 0.001} ${(-3.125 - 0.5) * 0.001} 0" rpy="0 0 0"/>
  <parent link="left_motor_mount"/>
  <child link="left_wheel"/>
  <axis xyz="0.0 -1.0 0.0"/>
</joint>

